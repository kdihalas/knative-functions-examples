{
  "listeners": {
      "*:8080": {
          "pass": "applications/function_index"
      }
  },
  "applications": {
      "function_index": {
          "type": "external",
          "working_directory": "/www/",
          "executable": "func.js",
          "user": "www-data",
          "group": "www-data",
      }
  }
}
