{
  "listeners": {
      "*:8080": {
          "pass": "applications/function_index"
      }

  },
  "applications": {
      "function_index": {
          "type": "external",
          "working_directory": "/tmp/",
          "executable": "/bin/func",
          "user": "www-data",
          "group": "www-data",
          "environment": {
            "GIN_MODE": "release"
          }
      }
  }
}
